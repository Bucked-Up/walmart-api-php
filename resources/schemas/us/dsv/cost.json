    "info": {
        "title": "Cost Management",
        "description": "This API allows DSV to update cost for items in bulk.",
        "version": "v3"
    },
    "servers": [
        {
            "url": "https:\/\/api-gateway.walmart.com",
            "description": "Production URL"
        }
