{
    "name": "highsidelabs/walmart-api",
    "description": "A PHP client for Walmart's APIs",
    "type": "library",
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "Walmart\\": "generated/"
        }
    },
    "authors": [
        {
            "name": "Jesse Evers",
            "email": "jesse@jesseevers.com"
        }
    ],
    "require-dev": {
        "jane-php/open-api-3": "^7.5",
        "friendsofphp/php-cs-fixer": "^3.9"
    },
    "require": {
        "jane-php/open-api-runtime": "^7.5",
        "guzzlehttp/guzzle": "^7.4",
        "symfony/uid": "^6.1"
    },
    "scripts": {
        "download-schemas": "php utils/download-schemas.php",
        "generate": "php vendor/bin/jane-openapi generate"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
